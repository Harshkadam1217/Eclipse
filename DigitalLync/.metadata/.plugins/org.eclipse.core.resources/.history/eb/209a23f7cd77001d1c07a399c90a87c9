package com.this1.keyword;

class A{
	C c;//data member
	//constructor using field
	int b;
	String name;
	public A(C c,String name,int b){
		this.b=b;
		this.name=name;
		System.out.println(b);
		System.out.println(name);
	}
	
	
}

class C{
	int b=1217;
	String name="Harsh";
	
	public C() {
		new A(this, name, b);
	}
}

public class Eg5 {

	public static void main(String[] args) {
		new C();

	}

}
