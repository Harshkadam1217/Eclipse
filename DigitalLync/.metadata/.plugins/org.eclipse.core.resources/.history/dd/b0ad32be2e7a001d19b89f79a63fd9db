package com.dl.two;
class A{
	public A() {
		System.out.println("Default constructor");
	}
	
	public A(int i) {
		System.out.println("Constructor with a parameters");
	}
}

class B extends A{
	public B() {
		super(10);
		System.out.println("B constructor");
	}
	
}

public class Eg1 {

	public static void main(String[] args) {
		new B();
	}

}
